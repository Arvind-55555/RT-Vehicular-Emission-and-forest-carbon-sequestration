{
  "version": 2,
  "name": "urban-emission-ai",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "installCommand": "npm install",
  "functions": {
    "api/**/*.py": {
      "maxDuration": 30
    }
  }
}